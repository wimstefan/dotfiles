{
  "$schema": "https://raw.githubusercontent.com/sumneko/vscode-lua/master/setting/schema.json",
  "Lua.diagnostics.disable": [
    "lowercase-global",
    "unused-local"
  ],
  "Lua.format.defaultConfig": {
    "indent_style": "space",
    "indent_size": "2",
    "continuation_indent_size": "2",
    "quote_style": "single"
  }
}

