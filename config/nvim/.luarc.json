{
  "$schema": "https://raw.githubusercontent.com/sumneko/vscode-lua/master/setting/schema.json",
  "Lua.diagnostics.disable": [
    "assign-type-mismatch",
    "param-type-mismatch",
    "redundant-parameter",
    "undefined-field",
    "undefined-global",
    "unused-local",
    "deprecated"
  ],
  "Lua.format.defaultConfig": {
    "indent_style": "space",
    "indent_size": "2",
    "continuation_indent_size": "2",
    "quote_style": "single"
  }
}
